    "scripts": {
        "prepublish":  "npm run upd",
        "dev":         "grunt",
        "start":       "grunt prod",
        "upd":         "upd -n",
        "policy":      "codingpolicy -r ./src codingpolicy.yaml"
    },
    "lite-proxyserver": {
        "mock": {
            "ctx":     "/mockdata",
            "file":    "./mock/rest-services.js"
        },
        "proxy": {
            "targetHosts": {
                "TEST": {"host": "unknown.msg.de", "port": 8080},
                "PROD": {"host": "unknown.msg.de", "port": 8081}
            },
            "target": "TEST",
            "port":   5678,
            "base":   "www",
            "proxies": [
                {
                    "context": "/backend",
                    "https": false,
                    "changeOrigin": false,
                    "rewrite": {
                        "^/backend": "/backend"
                    }
                }
            ]
        }
    }
}